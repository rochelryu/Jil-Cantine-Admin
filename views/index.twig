   <script>
	   $(document).ready(function () {
		   $('.table-bordered').DataTable({
			   "dom": '<"toolbar">frtip',
			   "paging": true,
			   "pagingType": "full_numbers"
		   });
	   })
   </script>
</body>
</html>
